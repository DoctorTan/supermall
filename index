# 混合代理端口 http(s)+socks
# mixed-port: 7890
# allow-lan: true   #允许局域网连接
bind-address: "*"
mode: rule
geodata-mode: true
log-level: info
ipv6: false
socks-port: 1080
# 其他部分别动，只需根据自己运营商修改百度IP
# DNS配置
dns:
  enable: true
  ipv6: false
  listen: 0.0.0.0:1053
  enhanced-mode: fake-ip
  fake-ip-range: 28.0.0.1/8
  default-nameserver:
    - https://223.6.6.6/dns-query
    - https://1.1.1.1/dns-query
  nameserver:
    - https://223.6.6.6/dns-query
    - https://1.1.1.1/dns-query

