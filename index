    skip-cert-verify: true
    password: 1234567
    up: "30 Mbps" # 若不写单位，默认为 Mbps
    down: "200 Mbps" # 若不写单位，默认为 Mbps 
  

  - name: 🇺🇸洛杉矶备用(明年7月)                            #可自定义名称
    type: hysteria2 
    server: xui.oortaka.top                            #解析的域名
    port: 49088                                        #自定义的端口
