    network: ws
    ws-opts: 
      path: /
 
 



proxy-groups:
# 节点群组，节点和订阅的名称配置到下面
  - name: 国外
    type: select
    proxies:
      - 🇺🇸洛杉矶(明年8月) 
      - 🇺🇸洛杉矶备用(明年7月)
      - 🇺🇸洛杉矶备用(一个月)  
      - DIRECT
    
      
# 如果想自动选择就把url-test改为select
    use: 
      - 机0
      - 机5

# 多久测试一次最快的节点,单位为秒,不想频繁换ip就把下面的删掉
    interval: 3000
  - name: 国内
    type: select 
    proxies:
      - DIRECT
      - 自动
      - 百度直连-广州电信
      - 百度直连-南京电信1
      - 百度直连-南京电信2
      - 百度直连-南京联通
      - 百度直连-苏州电信
      - 百度直连-苏州联通
      

  - name: 直连
    type: select 
    proxies:
      - DIRECT

  - name: 拦截
    type: select 
    proxies:
      - REJECT

  - name: 中继
    type: relay
    proxies:
      - 国内
      - 国外

rule-providers:
  reject:
    type: http
    behavior: domain
    url: "https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/reject.txt"
    path: ./ruleset/reject.yaml
    interval: 86400

rules:
  # UDP 规则 
  - AND,((NETWORK,UDP)),拦截
  # 广告过滤 
  - RULE-SET,reject,拦截
  - DOMAIN-KEYWORD,ad,拦截
  # 分流规则
  - DOMAIN-KEYWORD,google,中继
  - GEOSITE,CN,国内
  - GEOIP,CN,国内
  - MATCH,中继
