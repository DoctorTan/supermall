rule-providers:
  reject:
    type: http
    behavior: domain
    url: "https://cdn.jsdelivr.net/gh/Loyalsoldier/clash-rules@release/reject.txt"
    path: ./ruleset/reject.yaml
    interval: 86400

rules:
  # UDP 规则 
  - AND,((NETWORK,UDP)),拦截
  # 广告过滤 
  - RULE-SET,reject,拦截
  - DOMAIN-KEYWORD,ad,拦截
  # 分流规则
  - DOMAIN-KEYWORD,google,中继
  - GEOSITE,CN,国内
  - GEOIP,CN,国内
  - MATCH,中继
